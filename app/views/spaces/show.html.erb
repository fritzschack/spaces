<%= render "./components/navbar" %>
<div class="container">
  <h1><%= @space.name %></h1>
  <h3>$<%= @space.price_per_day %></h3>
  <h4><%= @space.address %> | <%= @space.phone %> | <%= link_to "Website", "https://#{@space.website}/", target: :_blank %></h4>
  <h5><%= @space.description %></h5>
  <%= link_to "Edit", edit_space_path %> | <%= link_to "Delete", space_path(@space), method: :delete %>
</div>
